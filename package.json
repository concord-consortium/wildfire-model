{
  "name": "wildfire-model",
  "version": "1.0.0",
  "description": "Concord Consortium starter projects",
  "main": "index.js",
  "jest": {
    "testURL": "https://wildfire-model.unexisting.url.com",
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/utilities/test-utils.ts"
    ],
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot --content-base dist/ --host 0.0.0.0",
    "build": "npm-run-all lint:build clean build:webpack",
    "build:webpack": "webpack --mode production --devtool false",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:build": "tslint -c tslint-build.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "tslint -c tslint-build.json --fix 'src/**/*.{ts,tsx}'",
    "lint:unused": "tsc --noUnusedLocals --project .",
    "test": "jest",
    "test:all": "npm-run-all build test start",
    "test:coverage": "jest --coverage",
    "test:debug": "node --nolazy --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache",
    "test:watch": "jest --watch",
    "test:coverage:watch": "jest --coverage --watch",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:full": "npm-run-all test test:cypress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/wildfire-model.git"
  },
  "author": "Concord Consortium",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/wildfire-model/issues"
  },
  "homepage": "https://github.com/concord-consortium/wildfire-model#readme",
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.9.1",
    "@svgr/webpack": "^5.3.0",
    "@types/chart.js": "^2.9.16",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.4",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@types/uuid": "^7.0.2",
    "autoprefixer": "^9.7.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^25.2.3",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cypress": "7.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "html-webpack-plugin": "^4.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "script-loader": "^0.7.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^4.2.0",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.8.3",
    "url-loader": "^4.0.0",
    "wait-on": "^4.0.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@concord-consortium/lara-interactive-api": "^1.1.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "chart.js": "^2.9.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-datalabels": "^0.7.0",
    "jquery": "^3.4.1",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.8",
    "patternomaly": "^1.3.2",
    "query-string": "^6.11.1",
    "rc-slider": "^8.7.1",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-circular-input": "^0.2.3",
    "react-dom": "^16.13.1",
    "react-three-fiber": "^4.0.23",
    "screenfull": "^5.0.2",
    "shutterbug": "^1.2.0",
    "three": "^0.125.0"
  }
}
